{
    "_metadata": {
        "major_version": 1,
        "minor_version": 1
    },
    "display_information": {
        "name": "Kit",
        "profile_picture": "https://cdn.hackclub.com/019c3add-359f-732c-94da-64eb92507428/jester.jpeg"
    },
    "features": {
        "app_home": {
            "home_tab_enabled": false,
            "messages_tab_enabled": true,
            "messages_tab_read_only_enabled": true
        },
        "bot_user": {
            "display_name": "Kit",
            "always_online": true
        },
        "slash_commands": [
            {
                "command": "/deathbyai",
                "description": "Play a game of Death by AI!",
                "should_escape": true
            },
            {
                "command": "/deathrespond",
                "description": "Respond to a Death by AI game",
                "should_escape": true
            },
            {
                "command": "/playRPS",
                "description": "A regular game of Rock, Paper, Scissors",
                "should_escape": true
            },
            {
                "command": "/playOmniRPS",
                "description": "Rock, Paper, Scissors, but you can use anything",
                "should_escape": true
            },
            {
                "command": "/messytext",
                "description": "Scrambles up your text",
                "should_escape": true
            },
            {
                "command": "/whispertext",
                "description": "Send an Only Visible to You message to anyone",
                "should_escape": true
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "chat:write",
                "chat:write.customize",
                "groups:read",
                "channels:write.invites", 
                "groups:write.invites", 
                "channels:manage", 
                "groups:write", 
                "im:write", 
                "chat:write.public",
                "datastore:read",
                "datastore:write",
                "channels:read",
                "commands",
                "users:read",
                "reactions:write",
                "bookmarks:read", 
                "bookmarks:write", 
                "canvases:read", 
                "canvases:write", 
                "channels:write", 
                "dnd:read",
                "emoji:read", 
                "links:read",
                "lists:read", 
                "lists:write", 
                "pins:read", 
                "pins:write", 
                "reactions:read", 
                "usergroups:read", 
                "usergroups:write", 
                "users.profile:read", 
                "users:write",
                "search:read.public",
                "profile"
            ],
            "user": [
                "dnd:write",
                "users.profile:write"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "member_joined_channel",
                "member_left_channel",
                "message.channels",
                "message.groups"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": true,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}
